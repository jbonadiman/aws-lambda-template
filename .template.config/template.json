{
  "$schema": "http://json.schemastore.org/template",
  "author": "João Vitor Bonadiman",
  "name": "Default Lambda",
  "description": "A starting-point AWS Lambda project with dependency injection functionality and basic NHibernate helper.",
  "identity": "JoaoBonadiman.DefaultLambda.1.0",
  "shortName": "defaultlambda",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "DefaultLambda",
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "netcoreapp2.1"
        },
        {
          "choice": "netcoreapp3.1"
        }
      ],
      "defaultValue": "netcoreapp3.1"
    },
    "AddDependencyInjection": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true"
    },
    "AddDatabase": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true"
    },
    "UsePostgres": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false"
    }
  },
  "sources": [
    {
      "exclude": [
        "**/[Bb]in/**",
        "**/[Oo]bj/**",
        ".template.config/**/*",
        "**/*.filelist",
        "**/*.user",
        "**/*.lock.json",
        ".vs/**",
        ".idea/**",
        ".git/**",
        "LICENSE.md"
      ],
      "modifiers": [
        {
          "condition": "(!AddDependencyInjection)",
          "exclude": [
            "DependencyInjection/**",
            "**/I*.cs"
          ]
        },
        {
          "condition": "(!AddDatabase)",
          "exclude": [
            "Database/INHibernateHelper.cs",
            "Database/NHibernateHelper.cs",
            "Entities/**",
            "Repositories/**"
          ]
        }
      ]
    }
  ],
  "classifications": [
  ]
}